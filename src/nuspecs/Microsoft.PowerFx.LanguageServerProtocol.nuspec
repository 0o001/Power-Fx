<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <!-- 
    This is a custom nuspec file
    - version or dependency/version are set to "****" when they need to be replaced with the real version number
    - files contain include/exclude elements to select which files need to be included/excluded in the package
      when no include/exclude, all files in 'Release' folder will be included
  -->
  <metadata>
    <id>Microsoft.PowerFx.LanguageServerProtocol</id>
    <version>****</version>
    <title>Microsoft Power Fx LanguageServerProtocol</title>
    <authors>Microsoft</authors>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <icon>icon.png</icon>
    <projectUrl>https://github.com/microsoft/Power-Fx</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/microsoft/Power-Fx/master/icon.png</iconUrl>
    <description>The LanguageServerProtocol of the Power Fx compiler. See more from https://microsoft.github.io/language-server-protocol/</description>
    <copyright>© Microsoft Corporation. All rights reserved.</copyright>
    <tags>powerfx</tags>
    <repository type="git" url="https://github.com/microsoft/Power-Fx" commit="****" />
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.PowerFx.Core" version="****" exclude="Build,Analyzers" />
        <dependency id="System.Text.Json" version="5.0.2" exclude="Build,Analyzers" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="Microsoft.PowerFx.Core" version="****" exclude="Build,Analyzers" />
        <dependency id="System.Text.Json" version="7.0.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework="net7.0">
        <dependency id="Microsoft.PowerFx.Core" version="****" exclude="Build,Analyzers" />
        <dependency id="System.Text.Json" version="7.0.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Add files here with absolute path, from src\libraries\Microsoft.PowerFx.LanguageServerProtocol\bin\Release\[netstandard2.0|net6.0|net7.0] (folder & subfolders) -->
    <!-- + <file src="src\libraries\Microsoft.PowerFx.LanguageServerProtocol\icon.png" target="\icon.png" /> -->
    <include>Microsoft.PowerFx.LanguageServerProtocol.*</include>
  </files>
</package>